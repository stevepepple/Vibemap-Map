    "body-parser": "^1.19.0",
    "chroma-js": "^2.1.0",
    "concurrently": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.17.3",
    "js-yaml": "^3.13.1",
    "moment": "^2.24.0",
    "mongoose": "^5.8.9",
    "nodemon": "^1.19.4",
    "path": "^0.12.7",
    "react-addons-shallow-compare": "^15.6.2",
    "react-map-gl": "^5.2.2",
    "request": "^2.88.0",
    "yarn": "^1.21.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": "10.x"
  },
  "browserslist": [
    ">0.2%",
